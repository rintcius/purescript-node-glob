{
  "name": "purescript-node-glob",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "output"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rintcius/purescript-node-glob.git"
  },
  "scripts": {
    "postinstall": "bower install",
    "clean": "rimraf bower_components && rimraf output && rimraf .psa-stash",
    "build": "pulp build -- --censor-lib --strict",
    "test": "pulp test"
  },
  "dependencies": {
    "glob": "isaacs/node-glob#v7.1.3"
  },
  "devDependencies": {
    "bower": "^1.8.8",
    "pulp": "^12.4.2",
    "purescript": "^0.12.5",
    "purescript-psa": "^0.7.3",
    "rimraf": "^2.6.3"
  }
}
